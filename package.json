{
  "name": "BenchWorx",
  "productName": "BenchWorx",
  "version": "0.0.1-beta.1",
  "description": "Desktop client for DynamoDB that supports Credentials, SSO, and MFA Role assumption",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "test": "jest",
    "cy": "cypress run",
    "cy:u": "cypress run --env updateSnapshots=true",
    "pub": "electron-forge publish",
    "lint": "eslint --ext .ts --ext .tsx .",
    "prepare": "husky install",
    "pub:beta": "release-it --preRelease=beta --no-npm.publish"
  },
  "lint-staged": {
    "*.ts|*.tsx": "yarn lint"
  },
  "keywords": [
    "DynamoDB",
    "GUI"
  ],
  "author": {
    "name": "Simon Reilly",
    "email": "simonireilly@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "simonireilly",
              "name": "dynamoDB-benchworx"
            },
            "draft": true,
            "prerelease": true
          }
        }
      ],
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "dynamodb_benchworx"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/preload.ts"
                  }
                }
              ]
            },
            "port": 3001,
            "loggerPort": 9001
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.2",
    "@cypress/react": "^4.16.4",
    "@cypress/webpack-dev-server": "^3.2.3",
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "6.0.0-beta.54",
    "@electron-forge/publisher-github": "^6.0.0-beta.54",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@release-it/keep-a-changelog": "^2.2.2",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.3",
    "@types/cypress-image-snapshot": "^3.1.5",
    "@types/jest": "^26.0.22",
    "@types/nock": "^11.1.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-plugin-istanbul": "^6.0.0",
    "css-loader": "^5.2.2",
    "cypress": "^6.8.0",
    "cypress-image-snapshot": "^4.0.1",
    "electron": "12.0.4",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "nock": "^13.0.11",
    "node-loader": "^2.0.0",
    "prettier": "^2.2.1",
    "release-it": "^14.6.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.5",
    "ts-loader": "^8.1.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.13.0",
    "@aws-sdk/client-sts": "^3.11.0",
    "@aws-sdk/credential-provider-node": "^3.13.0",
    "@aws-sdk/lib-dynamodb": "^3.11.0",
    "@material-ui/core": "^4.11.3",
    "@material-ui/data-grid": "^4.0.0-alpha.25",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@monaco-editor/react": "^4.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-util": "^0.15.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
